            <string>/// Navigation by arrow keys

if (alarm[1] = -1 )
    {
    switch(keyboard_key)
        {
        case(vk_nokey):
            alarm[1] = 0;
            break;
        
        case vk_up :
            if ( posY &gt; 0 ) y = y  - 24 ;
            alarm[1] =key_delay;
            break;
        
        case vk_down :
            if ( posY &lt; global.mapH - 1 ) y = y  + 24 ;
            alarm[1] =key_delay;
            break;
        
        case vk_left :
            if ( posX &gt; 0 ) x = x  - 24 ;
            alarm[1] =key_delay;
            break;      
                    
        case vk_right :
            if ( posX &lt; global.mapW - 1 ) x = x  + 24 ;
            alarm[1] =key_delay;
            break;      
          
        }
    }
</string>
